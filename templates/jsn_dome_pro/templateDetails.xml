	<config>
		<fields name="params" addfieldpath="/templates/jsn_dome_pro/elements">
			<fieldset name="about" label="ABOUT">
				<field name="jsnabout" type="jsnabout" description="" label="" />
			</fieldset>
			
			<fieldset name="sample-data" label="GETTING_STARTED">
				<field name="jsn-sample-data" type="jsnsampledata" description="" label="" />
			</fieldset>
			
			<fieldset name="logo-settings" label="LOGO_SETTINGS">
				<field name="logoPath" size="60" type="jsnmedia" default="templates/jsn_dome_pro/images/logo.png" label="LOGO_FILE" description="LOGO_FILE_DESC" />
				<field name="logoLink" size="60" type="text" default="index.php" label="LOGO_LINK" description="LOGO_LINK_DESC" />
				<field name="logoSlogan" size="60" type="text" default="" label="LOGO_SLOGAN" description="LOGO_SLOGAN_DESC" />
				<field name="enableColoredLogo" type="radio" default="0" label="ENABLE_COLORED_LOGO" description="ENABLE_COLORED_LOGO_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
			</fieldset>
			
			<fieldset name="layout-settings" label="LAYOUT_SETTINGS">
				<field type="spacer" default="" label="OVERALL_WIDTH" description=""/>
				<field name="templateWidth" type="list" default="narrow" label="TEMPLATE_WIDTH" description="TEMPLATE_WIDTH_DESC">
					<option value="narrow">NARROW</option>
					<option value="wide">WIDE</option>
					<option value="float">FLOAT</option>
				</field>
				<field name="narrowWidth" type="jsntext" size="6" class="jsn-input-number" default="960" label="NARROW_WIDTH" description="NARROW_WIDTH_DESC" posttext="px" />
				<field name="wideWidth" type="jsntext" size="6" class="jsn-input-number" default="1150" label="WIDE_WIDTH" description="WIDE_WIDTH_DESC" posttext="px" />
				<field name="floatWidth" type="jsntext" size="6" class="jsn-input-number" default="90" label="FLOAT_WIDTH" description="FLOAT_WIDTH_DESC" posttext="%"/>
				<field type="spacer" default="" label="COLUMNS_WIDTH" description="" />
				<field name="promoLeftWidth" type="jsntext" size="6" class="jsn-input-number" default="23" label="PROMO_LEFT_WIDTH" description="PROMO_LEFT_WIDTH_DESC" posttext="%"/>
				<field name="promoRightWidth" type="jsntext" size="6" class="jsn-input-number" default="23" label="PROMO_RIGHT_WIDTH" description="PROMO_RIGHT_WIDTH_DESC" posttext="%"/>
				<field name="leftWidth" type="jsntext" size="6" class="jsn-input-number" default="23" label="LCOL_WIDTH" description="LCOL_WIDTH_DESC" posttext="%"/>
				<field name="rightWidth" type="jsntext" size="6" class="jsn-input-number" default="23" label="RCOL_WIDTH" description="RCOL_WIDTH_DESC" posttext="%"/>
				<field name="innerleftWidth" type="jsntext" size="6" class="jsn-input-number" default="28" label="INNER_LCOL_WIDTH" description="INNER_LCOL_WIDTH_DESC" posttext="%"/>
				<field name="innerrightWidth" type="jsntext" size="6" class="jsn-input-number" default="28" label="INNER_RCOL_WIDTH" description="INNER_RCOL_WIDTH_DESC" posttext="%"/>
				<field type="spacer" default="" label="MISC" description=""/>
				<field name="showFrontpage" type="radio" default="1" label="SHOW_FRONTPAGE" description="SHOW_FRONTPAGE_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
			</fieldset>
			
			<fieldset name="mobile" label="MOBILE_SETTINGS">
				<field name="enableMobileSupport" type="jsnradio" default="1" label="MOBILE_SUPPORT" description="MOBILE_SUPPORT_DESC" cachesensitive="yes">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="enableMobileMenuSticky" type="jsnradio" default="1" label="MOBILE_MENU_STICKY" description="MOBILE_MENU_STICKY_DESC" cachesensitive="yes">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="showDesktopSwitcher" type="jsnradio" default="1" label="DESKTOP_SWITCHER" description="DESKTOP_SWITCHER_DESC" cachesensitive="yes">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="mobileLogoPath" size="60" type="jsnmedia" default="" label="MOBILE_LOGO_FILE" description="MOBILE_LOGO_FILE_DESC" cachesensitive="yes" />
			</fieldset>
			
			<fieldset name="style_settings" label="STYLE_SETTINGS">
				<field name="templateColor" type="list" default="red" label="TEMPLATE_COLOR" description="TEMPLATE_COLOR_DESC">
					<option value="orange">ORANGE</option>
					<option value="red">RED</option>
					<option value="cyan">CYAN</option>
					<option value="green">GREEN</option>
					<option value="yellow">YELLOW</option>
					<option value="pink">PINK</option>
				</field>
				<field name="templateTextStyle" type="list" default="personal" label="TEMPLATE_TEXTSTYLE" description="TEMPLATE_TEXTSTYLE_DESC">
					<option value="business">BUSINESS_CORPORATE_WEBSITE</option>
					<option value="personal">PERSONAL_BLOG_WEBSITE</option>
					<option value="news">ONLINE_NEWS_MAGAZINE_WEBSITE</option>
				</field>
				<field name="templateTextSize" type="list" default="medium" label="TEMPLATE_TEXTSIZE" description="TEMPLATE_TEXTSIZE_DESC">
					<option value="small">SMALL</option>
					<option value="medium">MEDIUM</option>
					<option value="big">BIG</option>
				</field>
				<field name="templateSpecialFont" type="radio" default="1" label="TEMPLATE_SPECIALFONT" description="TEMPLATE_SPECIALFONT_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="enableCSS3Effect" type="radio" default="1" label="CSS3_EFFECT" description="CSS3_EFFECT_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
			</fieldset>
			
			<fieldset name="menu_sitetools" label="ME_SETTINGS">
				<field type="spacer" default="" label="MENU" description=""/>
				<field name="mmWidth" type="jsntext" size="6" class="jsn-input-number" default="185" label="MM_WIDTH" description="MM_WIDTH_DESC" posttext="px" />
				<field name="smWidth" type="jsntext" size="6" class="jsn-input-number" default="185" label="SM_WIDTH" description="SM_WIDTH_DESC" posttext="px" />
				<field type="spacer" default="" label="SITETOOLS" description=""/>
				<field name="sitetoolsPresentation" type="jsnlist" default="menu" label="SITETOOLS_PRESENTATION" description="SITETOOLS_PRESENTATION_DESC" cachesensitive="yes">
					<option value="inline">INLINE</option>
					<option value="menu">MENU</option>
				</field>
				<field name="enableTextSizer" type="jsnradio" default="1" label="TEXT_SIZER" description="TEXT_SIZER_DESC" cachesensitive="yes">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="enableWidthSelector" type="jsnradio" default="1" label="WIDTH_SELECTOR" description="WIDTH_SELECTOR_DESC" cachesensitive="yes">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="enableColorSelector" type="jsnradio" default="1" label="COLOR_SELECTOR" description="COLOR_SELECTOR_DESC" cachesensitive="yes">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="colorIcons" size="60" type="jsntext" default="orange,red,cyan,green,yellow,pink" label="COLOR_ICONS" description="COLOR_ICONS_DESC" cachesensitive="yes"/>
			</fieldset>
			
			<fieldset name="seo_system" label="SEO_SYSTEM_SETTINGS">
				<field type="spacer" default="" label="SEO_ACCESSIBILITY" description=""/>
				<field name="enableTopH1" type="radio" default="1" label="TOP_H1" description="TOP_H1_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="enableGotopLink" type="radio" default="1" label="GOTOP_LINK" description="GOTOP_LINK_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="enableIconLinks" type="radio" default="0" label="AUTO_ICON" description="AUTO_ICON_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field name="enablePrintingOptimization" type="radio" default="0" label="PRINTING_OPTIMIZATION" description="PRINTING_OPTIMIZATION_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
				<field type="spacer" default="" label="SYSTEM" description=""/>
				<field name="analyticsCodePosition" type="list" default="1" label="ANALYTICS_CODE_POSITION" description="ANALYTICS_CODE_POSITION_DESC">
					<option value="0">BEFORE_ENDING_HEAD_TAG</option>
					<option value="1">BEFORE_ENDING_BODY_TAG</option>
				</field>
				<field name="analyticsCode" cols="30" rows="3" type="textarea" default="" filter="raw" label="ANALYTICS_CODE" description="ANALYTICS_CODE_DESC"/>
				<field name="customCSS" cols="30" rows="3" type="textarea" default="" label="CUSTOM_CSS" description="CUSTOM_CSS_DESC"/>
				<field name="cssJsCompression" type="list" default="0" label="CSS_JS_COMPRESSION" description="CSS_JS_COMPRESSION_DESC">
					<option value="0">NO_COMPRESSION</option>
					<option value="1">BOTH_CSS_JS_COMPRESSION</option>
					<option value="2">CSS_COMPRESSION_ONLY</option>
					<option value="3">JS_COMPRESSION_ONLY</option>
				</field>
				<field name="cacheFolder" type="jsncachefolder" size="60" default="tmp" label="CACHE_FOLDER" description="CACHE_FOLDER_DESC" phpversion="5.1.6"/>
				<field name="enableSqueezebox" type="radio" default="0" label="SQUEEZEBOX" description="SQUEEZEBOX_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
				</field>
			</fieldset>
		</fields>
	</config>
</extension>