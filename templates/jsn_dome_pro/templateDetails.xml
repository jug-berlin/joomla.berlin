		<language tag="de-DE">de-DE/de-DE.tpl_jsn_dome_pro.ini</language>
		<language tag="de-DE">de-DE/de-DE.tpl_jsn_dome_pro.sys.ini</language>
		<language tag="nl-NL">nl-NL/nl-NL.tpl_jsn_dome_pro.ini</language>
